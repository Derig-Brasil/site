{
  "name": "Derig - Site",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "runArgs": ["-e", "FORCE_COLOR=1"],
  "postCreateCommand": "npm install",
  "forwardPorts": [8080, 8787],
  "features": {},
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "dbaeumer.vscode-eslint"
      ]
    }
  }
}
